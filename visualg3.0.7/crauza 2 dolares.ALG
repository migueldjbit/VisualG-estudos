Algoritmo "semnome"

Var
     total, valor, dolar: real

Inicio
      escreva ("quantos Reais Você tem para saber quantos dolares você pode comprar")
              leia (valor)
      escreva ("quanto esta o dolar hoje")
              leia (dolar)
              total <- valor * dolar
      escreva ("Você pode comprar ", total ," de dolares para sua viagem" )

Fimalgoritmo